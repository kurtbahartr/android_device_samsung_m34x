    <!-- Status Bar. -->
    <dimen name="status_bar_height_default">24.0dp</dimen>
    <dimen name="status_bar_height_landscape">24.0dp</dimen>
    <dimen name="status_bar_height_portrait">24.0dp</dimen>
    <dimen name="status_bar_padding_top">2dp</dimen>

    <!-- Rounded Corners. -->
    <dimen name="rounded_corner_radius_bottom">36.0dp</dimen>
    <dimen name="rounded_corner_radius_top">36.0dp</dimen>
    <dimen name="rounded_corner_content_padding">52.0px</dimen>

    <!-- Physical power button location. -->
    <dimen name="physical_power_button_center_screen_location_y">1000px</dimen>

    <!-- Physical fingerprint sensor location. -->
    <dimen name="physical_fingerprint_sensor_center_screen_location_x">1080px</dimen>
    <dimen name="physical_fingerprint_sensor_center_screen_location_y">1000px</dimen>
</resources>
